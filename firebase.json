{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source": "**/*.@(jpg|jpeg|gif|png|ico|svg|webp|mp4)",
      "headers": [{
          "key": "Cache-Control",
          "value": "max-age=2628000"
      }]
    }]
  }
}
